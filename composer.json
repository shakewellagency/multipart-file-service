{
    "name": "shakewellagency/multipart-file-service",
    "description": "A Laravel package for handling multipart file uploads via S3.",
    "keywords": ["laravel", "upload", "s3", "multipart", "file-service"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shakewell Agency",
            "email": "info@shakewell.agency"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Services\\UploadService\\": "./"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\Services\\UploadService\\UploadServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
